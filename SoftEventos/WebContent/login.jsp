<form method="POST" action="AutenticarConta" id="login-form">

		<span id="erro"> ${erro} </span>

		<p>
			Login: <span>*</span> <br> 
			<input type="text" name="login" placeholder="Login">
		</p>

		<p>
			Senha: <span>*</span> <br> 
			<input type="password" name="senha" placeholder="Senha">
		</p>

		<button type="submit">Entrar</button>

</form>